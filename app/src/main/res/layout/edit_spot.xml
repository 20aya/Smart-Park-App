<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple_700">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="410dp"
        android:layout_height="348dp"
        android:layout_marginTop="180dp"
        android:background="#FFFEFC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView22op"
            android:layout_width="172dp"
            android:layout_height="138dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="50dp"
            android:clickable="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/addddddddd" />

        <TextView
            android:id="@+id/textViewere"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="84dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="25dp"
            android:paddingLeft="20dp"
            android:shadowColor="#9721CD"
            android:text="price"
            android:textColor="#8D2AB8"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/editTextNumbern21"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView22op" />

        <View
            android:id="@+id/view88ttj"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginBottom="5dp"
            android:background="#517A2C"
            app:layout_constraintBottom_toTopOf="@+id/textViewere"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/editTextNumbern21"
            android:layout_width="76dp"
            android:layout_height="36dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="10dp"
            android:ems="10"
            android:inputType="numberDecimal"
            android:paddingLeft="20dp"
            android:text="0.00"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/textViewsel"
            app:layout_constraintStart_toEndOf="@+id/textViewere"
            app:layout_constraintTop_toBottomOf="@+id/imageView22op" />

        <TextView
            android:id="@+id/textVieweren"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="93dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="12dp"
            android:paddingLeft="20dp"
            android:text="Type "
            android:textColor="#7B1FA2"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/switch21"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewere" />

        <androidx.appcompat.widget.SwitchCompat
            android:id="@+id/switch21"
            android:layout_width="108dp"
            android:layout_height="29dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="155dp"
            android:textOff="Outdoor"
            android:textOn="Indoor"
            android:thumb="@drawable/sw_thumb"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/textVieweren"
            app:layout_constraintTop_toBottomOf="@+id/editTextNumbern21"
            app:showText="true"
            app:track="@drawable/track" />

        <TextView
            android:id="@+id/textViewsel"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="40dp"
            android:text="per Hour/ SAR $"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView22op" />

        <Button
            android:id="@+id/button12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="15dp"
            android:text="save"
            app:layout_constraintEnd_toStartOf="@+id/button14"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switch21" />

        <Button
            android:id="@+id/button14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="13dp"
            android:text="Cancel"
            app:layout_constraintStart_toEndOf="@+id/button12"
            app:layout_constraintTop_toBottomOf="@+id/switch21" />

        <ImageView
            android:id="@+id/imageView22ovv"
            android:layout_width="160dp"
            android:layout_height="110dp"
            android:layout_marginTop="48dp"
            app:layout_constraintEnd_toStartOf="@+id/imageView22op"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ees22" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>